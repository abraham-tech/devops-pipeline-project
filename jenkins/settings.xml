<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    
    <servers>
        <!-- Artifactory deployment server -->
        <server>
            <id>artifactory</id>
            <username>admin</username>
            <password>password</password>
        </server>
    </servers>
    
    <mirrors>
        <mirror>
            <id>artifactory</id>
            <name>artifactory</name>
            <url>http://localhost:8082/artifactory/iwayq-libs-release</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>
    
    <profiles>
        <profile>
            <id>artifactory</id>
            <repositories>
                <repository>
                    <id>artifactory</id>
                    <name>iwayq-libs-release</name>
                    <url>http://localhost:8082/artifactory/iwayq-libs-release</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </repository>
                <repository>
                    <id>artifactory-snapshots</id>
                    <name>iwayq-libs-snapshot</name>
                    <url>http://localhost:8082/artifactory/iwayq-libs-snapshot</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>artifactory</id>
                    <name>iwayq-libs-release</name>
                    <url>http://localhost:8082/artifactory/iwayq-libs-release</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>artifactory-snapshots</id>
                    <name>iwayq-libs-snapshot</name>
                    <url>http://localhost:8082/artifactory/iwayq-libs-snapshot</url>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
    
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
    
</settings>
